# /etc/network/interfaces -- configuration file for LEAF network
# $Id: interfaces,v 1.3 2006/06/30 06:43:08 doros Exp $
#
# Loopback interface.
auto lo
iface lo inet loopback
#
auto eth1
iface eth1 inet static  
	address 10.10.20.3
	netmask 255.255.255.0
	broadcast 10.10.20.255
#
auto eth0
iface eth0 inet static
	address 212.1.2.1
	netmask 255.255.255.0
	broadcast 212.1.2.255
	gateway 212.1.2.2
#
# bot
